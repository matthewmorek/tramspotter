{
  "name": "tramspotter",
  "version": "0.5.1",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test": "jest",
    "test:precommit": "jest --findRelatedTests",
    "test:prepush": "jest --colors",
    "test:watch": "jest --watchAll",
    "lint": "vue-cli-service lint",
    "watch": "vue-cli-service build --watch",
    "storybook": "start-storybook -p 6006 --ci",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@babel/runtime": "^7.7.2",
    "@bugsnag/js": "^6.4.3",
    "@bugsnag/plugin-vue": "^6.4.1",
    "autoprefixer": "^9.7.1",
    "axios": "^0.19.0",
    "camelcase-keys": "^6.1.1",
    "cssnano": "^4.1.10",
    "date-fns": "^2.7.0",
    "lodash": "^4.17.15",
    "postcss": "^7.0.21",
    "postcss-cli": "^6.1.3",
    "postcss-import": "^12.0.1",
    "postcss-load-config": "^2.1.0",
    "precss": "^4.0.0",
    "register-service-worker": "^1.6.2",
    "reset-css": "^5.0.1",
    "uuid": "^3.3.3",
    "vue": "^2.6.10",
    "vue-browser-geolocation": "^1.8.0",
    "vue-wait": "^1.4.8",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/plugin-transform-runtime": "^7.7.4",
    "@babel/preset-env": "^7.7.1",
    "@babel/runtime-corejs2": "^7.7.2",
    "@storybook/addon-a11y": "^5.2.6",
    "@storybook/addon-actions": "^5.2.6",
    "@storybook/addon-knobs": "^5.2.6",
    "@storybook/addon-links": "^5.2.6",
    "@storybook/addon-storyshots": "^5.2.6",
    "@storybook/addon-storysource": "^5.2.6",
    "@storybook/addon-viewport": "^5.2.6",
    "@storybook/addons": "^5.2.6",
    "@storybook/vue": "^5.2.6",
    "@vue/cli-plugin-babel": "4.0.5",
    "@vue/cli-plugin-eslint": "4.0.5",
    "@vue/cli-plugin-pwa": "4.0.5",
    "@vue/cli-service": "4.0.5",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-macros": "^2.7.0",
    "babel-preset-vue": "^2.0.2",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^6.0.1",
    "husky": "^3.1.0",
    "install-peers": "^1.0.3",
    "jest": "^24.9.0",
    "jest-css-modules": "^2.1.0",
    "jest-serializer-vue": "^2.0.2",
    "jest-transform-stub": "^2.0.0",
    "jest-vue-preprocessor": "^1.7.0",
    "lint-staged": "^9.4.2",
    "nock": "^11.7.0",
    "postcss-loader": "^3.0.0",
    "require-context.macro": "^1.2.2",
    "storybook-addon-themes": "^5.2.0",
    "stylelint": "^12.0.0",
    "stylelint-config-prettier": "^6.0.0",
    "stylelint-prettier": "^1.1.1",
    "vue-cli-plugin-netlify-lambda": "^0.1.1",
    "vue-jest": "^3.0.5",
    "vue-svg-loader": "^0.15.0"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn test:prepush && lint-staged",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ],
    "*": [
      "yarn test:precommit"
    ]
  }
}
